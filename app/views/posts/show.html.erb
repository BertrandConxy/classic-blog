<p style="color: green"><%= notice %></p>

<%= render partial: 'shared/post', locals: {post: @post } %>

<div>
  <%= link_to "Edit this post", edit_user_post_path(@post) %> |
  <%= link_to "Back to posts", user_posts_path %>

  <%= button_to "Destroy this post", user_post_path(@post), method: :delete %>
</div>
